            <string>//Draw Self
script_execute(animType,1)

if mouse_wheel_up()
{
    legMult += .01
}
if mouse_wheel_down()
{
    legMult -= .01
}

//Draw UI
draw_set_font(fnt_dev)
draw_set_halign(fa_left)
draw_set_valign(fa_middle)
draw_set_colour(c_white)
draw_text(72,8,"R: "+string(race))
draw_text(72,16,"GW: "+string(greatWeapon))
draw_text(72,24,"H: "+string(height)+"px - "+string(height*6)+"cm")
switch(mode)
{
    case 1:
    draw_text(72,32,"Head Off: "+string(headOffset))
    draw_text(72,40,"Head Adj: "+string(headAdjust))
    draw_text(72,48,"Body: "+string(bodyImage))
    break
    
    case 2:
    draw_text(72,32,"Chest Adj: "+string(chstAdjust))
    draw_text(72,40,"Chest: "+string(chstImage))
    break
    
    case 3:
    draw_text(72,32,"Body Off: "+string(bodyOffset))
    draw_text(72,40,"Body Adj: "+string(bodyAdjust))
    draw_text(72,48,"Body: "+string(bodyImage))
    draw_text(72,56,"Hips: "+string(hipsImage))
    draw_text(72,64,"Chest: "+string(chstImage))
    break
    
    case 4:
    draw_text(72,32,"Leg Adj: "+string(legAdjust[1]))
    draw_text(72,40,"Hips: "+string(hipsImage))
    
    draw_text(72,48,"Leg %: "+string(legMult))
}
