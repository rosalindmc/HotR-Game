        {    
            i = instance_create(x-10,y-6,obj_thinwall)
            i.owner = id
            i.h = h
            i.facing = -1
            wall[3] = i
            if isoX &gt; 0
            {
                obj_control.map[isoX-1+(isoZ*obj_control.mapWidth),isoY].wall[1] = i
            }
        }
    }
}
else
