            <string>if isoZ &lt;= global.zLevel or global.zViewToggle = true
{
    if vis != false and scrollVis != false
    {
        if global.lR != lR or global.lG != lG or global.lB != lB or global.alpha != alpha
        {
            shader_set_uniform_f(global.shader_light, lR, lG, lB, alpha)
            global.lR = lR
            global.lG = lG 
            global.lB = lB 
            global.alpha = alpha
        }

        
        if wall[0] != noone
        {
            draw_sprite_ext(wall[0].sprite_index,wall[0].image_index,wall[0].x,wall[0].y-h+(global.zLevel*15),wall[0].facing,1,0,c_white,1)
        }
        if wall[3] != noone
        {
            draw_sprite_ext(wall[3].sprite_index,wall[3].image_index,wall[3].x,wall[3].y-h+(global.zLevel*15),wall[3].facing,1,0,c_white,1)
        }
        
        draw_sprite(sprite_index,image_index,x,y-h-wH+(global.zLevel*15))

        for(i=0;i&lt;5;i++)
        {
            draw_sprite_ext(level[i],i+(wave*5),x,y-floor(wave/4)-h-wH+(global.zLevel*15),1,1,0,trimColour,1)
        }
        
        for(i=0;i&lt;doodads;i++)
        {
            draw_sprite_ext(doodadSprite[i],doodadImage[i],doodadX[i],doodadY[i]-h+(global.zLevel*15),doodadFlip[i],1,0,c_white,1)
        }
        
        if  overlay != 0 || onPath &gt;= 4
        {
        
            if global.lR != (lR+1)/2 or global.lG != (lG+1)/2 or global.lB != (lB+1)/2 or global.alpha != 1
            {
                shader_set_uniform_f(global.shader_light, (lR+1)/2, (lG+1)/2, (lB+1)/2, 1)
                global.lR = (lR+1)/2
                global.lG = (lG+1)/2
                global.lB = (lB+1)/2
                global.alpha = 1
            }
        
            if isoZ = global.zLevel or onPath &gt;= 4
            {
                draw_sprite(spr_tilePathing,max(overlay,onPath),x,y-h-wH+(global.zLevel*15))
            }
            else 
            {
                draw_sprite(spr_tilePathing,overlay+5,x,y-h-wH+(global.zLevel*15))
            }
        }
        //if zoneControl = true 
